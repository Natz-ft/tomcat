}
.b-container .g-main .b-body .data-list .data-block > ul > li .d-list-title:before {
  content: "";
  position: absolute;
  left: -14px;
  top: 7px;
  width: 7px;
  height: 7px;
  border-radius: 50%;
  background-color: #2e5bc7;
}
