  margin: 20px auto;
  border: 1px solid #BDBDBD;
  border-radius: 4px;
  text-align: center;
  line-height: 40px;
  font-size: 16px;
  color: #444;
  cursor: pointer;
  transition: .3s;
}
.b-container .g-main .b-body .subj-list .show-all:hover {
  color: #1CB3F9;
  border: 1px solid #1CB3F9;
}
.b-container .g-main .b-body .data-list {
  margin-top: 20px;
  overflow: hidden;
}
.b-container .g-main .b-body .data-list .data-block {
  width: 48%;
  float: left;
}
.b-container .g-main .b-body .data-list .data-block .data-title-container {
  width: 100%;
  height: 60px;
  border-bottom: 1px solid #dcdcdc;
  box-sizing: content-box;
}
.b-container .g-main .b-body .data-list .data-block .data-title-container .title-name {
  position: relative;
  height: 61px;
  float: left;
  line-height: 60px;
  font-size: 24px;
  color: #333;
  border-bottom: 2px solid #2e5bc7;
}
.b-container .g-main .b-body .data-list .data-block .data-title-container .title-name:before {
  content: "";
  position: absolute;
  right: -18px;
  top: 26px;
  width: 0;
  height: 0;
  border-top: 5px solid transparent;
  border-right: 8px solid #fd4100;
  border-bottom: 5px solid transparent;
}
.b-container .g-main .b-body .data-list .data-block .data-title-container .title-name:after {
  content: '';
  position: absolute;
  right: -58px;
  top: 21px;
  width: 40px;
  height: 20px;
  background-color: #fd4100;
  font-size: 10px;
  color: #fff;
  text-align: center;
  line-height: 20px;
}
.b-container .g-main .b-body .data-list .data-block .data-title-container .title-name.new-more:after {
  content: "NEW";
}
.b-container .g-main .b-body .data-list .data-block .data-title-container .title-name.hot-more:after {
  content: "HOT";
}
.b-container .g-main .b-body .data-list .data-block .data-title-container .title-more {
  position: relative;
  float: right;
  padding-right: 15px;
  line-height: 60px;
  font-size: 12px;
  color: #999;
  font-family: Arial;
  cursor: pointer;
}
.b-container .g-main .b-body .data-list .data-block .data-title-container .title-more:after {
  content: "";
  position: absolute;
  right: 0;
  top: 25px;
  width: 0;
  height: 0;
  border-top: 5px solid transparent;
  border-left: 6px solid #999;
  border-bottom: 5px solid transparent;
}
.b-container .g-main .b-body .data-list .data-block > ul {
  width: 100%;
}
.b-container .g-main .b-body .data-list .data-block > ul > li {
