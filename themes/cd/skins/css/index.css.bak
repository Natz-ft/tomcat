@font-face {  
  font-family: 'siyuan';  
  src:url('./siyuan/siyuan.ttf') format('truetype');
  font-weight: normal;  
  font-style: normal;  
} 
.m-carousel {
  position: relative;
  height: 320px;
}
.m-carousel:after {
  content: '';
  position: absolute;
  z-index: 2;
  width: 440px;
  height: 51px;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  background: url('../img/index/imga.png') no-repeat;
}
.m-carousel .m-slogan {
  position: absolute;
  z-index: 2;
  left: 0;
  right: 0;
  top: 93px;
  text-align: center;
  font-size: 40px;
  color: #fff;
}
.m-carousel .m-slogan p {
  margin-top: 30px;
  font-size: 18px;
}
.m-carousel .m-slogan em {
  margin: 0 5px;
  font-style: normal;
  font-weight: bold;
  color: #fff000;
}
.m-carousel .carousel {
  height: 100%;
}
.m-carousel .carousel .caro-arrow div {
  z-index: 2;
}
.m-carousel .carousel .caro-tab {
  left: 48%;
  bottom: 61px;
}
.m-carousel .carousel .caro-tab ul li {
  width: 25px;
  height: 2px;
  background-color: #fff;
}
.m-carousel .carousel .caro-tab ul li.active {
  background-color: #00EFFE;
}
.m-carousel .caro-banner1 {
  background: url('../img/index/banner1.png') no-repeat center;
}
.m-carousel .caro-banner2 {
  background: url('../img/index/banner2.png') no-repeat center;
}
.b-container {
  width: 100%;
}
.b-container .g-main {
  margin: 0 auto;
  width: 1200px;
}
.b-container .g-main .b-head {
  position: relative;
  width: 100%;
  height: 60px;
  text-align: center;
  font-size: 34px;
  color: #333;
}
.b-container .g-main .b-head span {
  font-size: 16px;
  color: #999;
}
.b-container .g-main .b-head:after {
  content: '';
  position: absolute;
  width: 50px;
  height: 5px;
  bottom: 0;
  left: 50%;
  margin-left: -25px;
  background-color: #c8c8c8;
}
.b-container .g-main .b-body {
  width: 100%;
}
.b-container .g-main .b-body .subj-list {
  margin-top: 20px;
}
.b-container .g-main .b-body .subj-list > ul {
  height: 266px;
  overflow: hidden;
}
.b-container .g-main .b-body .subj-list > ul.mark {
  height: auto;
}
.b-container .g-main .b-body .subj-list > ul > li {
  position: relative;
  width: 96px;
  height: 133px;
  margin-left:67px;
  float: left;
  text-align: center;
}
.b-container .g-main .b-body .subj-list > ul > li a {
  display: block;
  width: 100%;
  height: 100%;
  padding-top: 80px;
}
.b-container .g-main .b-body .subj-list > ul > li a:hover .subj-name {
  color: #1b37a8;
  transition: .3s;
}
.b-container .g-main .b-body .subj-list > ul > li a:hover .subj-pic {
  top: 10px;
}
.b-container .g-main .b-body .subj-list > ul > li .subj-pic {
  position: absolute;
  left: 0;
  right: 0;
  top: 20px;
  width: 60px;
  height: 60px;
  margin: auto;
  background: url("../img/index/icona.png") no-repeat;
  transition: .5s;
}
